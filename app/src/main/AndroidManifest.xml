<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ilkomhotspot.teman">

    <uses-permission android:name="android.permission.VIBRATE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.ilkomhotspot.teman.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.ilkomhotspot.teman.MainActivity" />
        <activity android:name="com.ilkomhotspot.teman.Contact" />
        <activity android:name="com.ilkomhotspot.teman.EditAkun" />
        <activity android:name="com.ilkomhotspot.teman.Terjatuh"/>
        <activity android:name="com.ilkomhotspot.teman.fallDetector" />
    </application>

</manifest>